{
  "name": "google/cloud",
  "type": "library",
  "description": "Google Cloud Client Library",
  "keywords": [
    "google apis client",
    "google api client",
    "google apis",
    "google api",
    "google",
    "google cloud platform",
    "google cloud",
    "cloud",
    "bigquery",
    "big query",
    "stackdriver logging",
    "natural language",
    "pubsub",
    "pub sub",
    "storage",
    "gcs",
    "translate",
    "vision"
  ],
  "homepage": "http://github.com/GoogleCloudPlatform/google-cloud-php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Dave Supplee",
      "email": "dwsupplee@gmail.com"
    },
    {
      "name": "John Pedrie",
      "email": "john@pedrie.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "rize/uri-template": "~0.3",
    "google/auth": "0.9",
    "guzzlehttp/guzzle": "~5.2|~6.0",
    "guzzlehttp/psr7": "^1.2",
    "monolog/monolog": "~1",
    "psr/http-message": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "squizlabs/php_codesniffer": "2.*",
    "phpdocumentor/phpdocumentor": "2.8.*",
    "symfony/console": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Google\\Cloud\\": "src"
    }
  },
  "scripts": {
    "docs": "php scripts/docs.php"
  }
}
